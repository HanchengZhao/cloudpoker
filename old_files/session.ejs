                                    <%= name='tom' %>: <%= stack %>
                                </div>
                            </div>
                            <div class="space1"></div>
                            <div class="hand">
                                <%- include('../partials/card', { showCards, rank: 'Q', suit: '♥︎', color: 'red', action: false}); %>
                                <%- include('../partials/card', { showCards, rank:'Q', suit: '♦︎', color: 'red', action: false}); %>
                                <div class="name">
                                    <%= name='tom' %>: <%= stack %>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="bottom-middle-two">
                        <div class="centering">
                            <div class="hand">
                                <%- include('../partials/card', { showCards, rank: 'Q', suit: '♥︎', color: 'red', action: false}); %>
                                <%- include('../partials/card', { showCards, rank:'Q', suit: '♦︎', color: 'red', action: false}); %>
