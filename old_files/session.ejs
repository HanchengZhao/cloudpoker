                                    <%= name='tom' %>: <%= stack %>
                                </div>
                            </div>
                            <div class="space"></div>
                            <div class="hand">
                                <%- include('../partials/card', { showCards, rank: 'Q', suit: '♥︎', color: 'red', action: false}); %>
                                <%- include('../partials/card', { showCards, rank:'Q', suit: '♦︎', color: 'red', action: false}); %>
