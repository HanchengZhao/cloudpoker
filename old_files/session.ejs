                                    <%= name='tom' %>: <%= stack %>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="bottom-two">
                    <div class="centering">
                        <div class="hand">
                            <%- include('../partials/card', { showCards, rank: 'Q', suit: '♥︎', color: 'red', action: false}); %>
                            <%- include('../partials/card', { showCards, rank:'Q', suit: '♦︎', color: 'red', action: false}); %>
